{
  "version": 2,
  "name": "julian-cv",
  "alias": "cv.julianux.com",
  "public": true,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "./_next/static/(?:[^/]+/pages|chunks|runtime)/.+",
      "headers": {
        "cache-control": "immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/$1",
      "headers": {
        "x-request-path": "$1"
      }
    }
  ]
}
